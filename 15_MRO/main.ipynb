    "class A:\n",
    "    def show(self):\n",
    "        print(\"A class\")\n",
    "\n",
    "class B(A):\n",
    "    def show(self):\n",
    "        print(\"B class\")\n",
    "\n",
    "class C(A):\n",
    "    def show(self):\n",
    "        print(\"C class\")\n",
    "\n",
    "class D(B, C):  # Diamond inheritance\n",
    "    pass\n",
    "\n",
    "obj = D()\n",
    "obj.show()\n"
   ]
